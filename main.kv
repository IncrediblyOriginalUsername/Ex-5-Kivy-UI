<MainScreen>:
    name: 'main'
    egg: egg
    egt: egt
    lma: lma
    lmt: lmt
    sli: sli
    asd: asd
    FloatLayout:
        size_hint: None, None

    DPEAButton:
        id: test_button
        text: 'This is America'
        size: 100, 100
        x: root.width * 0.75
        y: root.height * 0.5
        color: 0, 1, 0, 1
        on_press: root.pressed()

    DPEAButton:
        text: "test2"
        size: 100, 100
        x: root.width * 0.25
        y:root.height * 0.5
        color: 1, 0, 0, 1
        on_press: root.pressed()

    DPEAButton:
        id: egg
        size: 100, 100
        x: root.width * 0.1
        y: root.height * 0.3

        color: 0.035, 0.152, 0.725, 1
        text: "On"



        on_press: root.changeText()
    DPEAButton:
        id: egt
        size: 100, 100
        x: root.width * 0.1
        y: root.height * 0.5

        color: 0.035, 0.152, 0.725, 1
        text: "0"



        on_press: root.counterButton()




    ImageButton:
        id: logo_image_button
        source: "../../pidev/kivy/images/DPEALogoTransparent.png"
        size: 100, 100
        x: root.width * 0.5
        y: root.height * 0.5
        on_press: root.pressed()
    Label:
        id: lma
        text: "Motor on"
        font_size: 30
        x: root.width * -.4
        y: root.height * -.3
        color: 0, 0, 0, 1
    DPEAButton:
        size: 250, 100
        x: root.width * 0.1
        y: root.height * .01

        color: 0.035, 0.152, 0.725, 1
        text: "Motor Controller"
        on_press: root.Motor()
    DPEAButton:
        size: 250, 100
        x: root.width * 0.8
        y: root.height * .01

        color: 0.535, 0.152, 0.95, 1
        text: "To JoyCon screen"
        on_press: root.fellow()
    ImageButton:
        source: "../../Desktop/Reinstalling_system.jpg"
        size: 250, 100
        x: root.width * .4
        y: root.height * .1

        color: 0.035, 0.152, 0.725, 1
        text: "Moo9"
        on_press: root.riseup()
    ImageButton:
        id:asd
        source: "../../Desktop/High_quality_GUI.jpg"
        size: 250, 100
        x: root.width * .9
        y: root.height * .1

        color: 0.035, 0.152, 0.725, 1
        text: "Moo9"
        on_press: root.gamers()





    Slider:
        size_hint_y: .7
        size_hint_x: .2
        id: sli
        min: 0
        max: 100
        step: 1
        orientation: "vertical"
        x: root.width *.3
        y: root.width * .15
        on_touch_move: root.up()


    Label:
        id: lmt
        text: "0"
        font_size: 30
        x: root.width * .01
        y: root.height * .2
        color: 0, 0, 0, 1
