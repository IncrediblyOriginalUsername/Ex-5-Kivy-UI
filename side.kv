<SideScreen>:
    name: 'side'
    yes: yes
    FloatLayout:
        size_hint: None, None
    ImageButton:
        id: test_button
        source: "../../Desktop/Tour_of_projects.jpg"
        text: 'This is America'
        size: 1000, 100
        x: root.width * 0
        y: root.height * 0.5
        color: 0, 1, 0, 1
        on_press: root.ye()
    TextInput:
        id: yes
        size_hint: None, None
        height: 100
        width: 100
        x: root.width * 0
        y: root.height *0
        text: "Change title"
        on_text: root.change()